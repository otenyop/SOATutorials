<?xml version="1.0" encoding="windows-1252" ?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns="http://soahost.co.ke/SOATutorial/CardValidationSystem/cvsSchema"
            targetNamespace="http://soahost.co.ke/SOATutorial/CardValidationSystem/cvsSchema"
            elementFormDefault="qualified">
  <xsd:element name="cvsRequest">
    <xsd:annotation>
      <xsd:documentation>A sample element</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="cardID" type="xsd:integer"/>
        <xsd:element name="vendorName" type="xsd:string"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="cvsResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="cardID" type="xsd:integer"/>
        <xsd:element name="vendorName" type="xsd:string"/>
        <xsd:element name="cardStatus" type="xsd:string"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
